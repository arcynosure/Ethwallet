                            <input type="text" placeholder="Mneumonics">
                        </div>
                        <button class="btn btn--blue btn--full">Restore from Seed</button>
                    </div>
                    <div id="tab-3" class="walletselect support__content-item support__chat">
                        <div class="input-wrap1">
                            <input name= "avatar" enctype = "multipart/form-data" type="file" placeholder="Select the keystore. ">
                        </div>
                        <div class="input-wrap">
                            <div class="form-group">
                                <input type="text" name="passwords" id="passwords" tabindex="1" class="form-control" placeholder="Password" value="">
                            </div>
                        </div>
                       
                       
                        <button type= submit formaction="/login" class="btn btn--blue btn--full">Import Keystore</button>
                    </div>
                </div>
            </div>

            <div class="popup__thanks">
                <img src="img/popup-success.png" alt="">
                <h3>Your wallet added successful!</h3>
            </div>
