                    <h3>Add new wallet</h3>
                    <ul class="support__tabs">
                        <li class="support__tabs-item active" data-tab="1">Create Wallet</li>
                        <li class="support__tabs-item" data-tab="2">Restore Wallet</li>
                        <li class="support__tabs-item" data-tab="3">Import Wallet</li>
                    </ul>
                </div>
                <div class="support__content">
                    <div id="tab-1" class="walletselect support__content-item support__faq active">
                        <div class="input-wrap">
                        </div>
                        <div class="input-wrap">
                            <div class="form-group">
                                <input type="text" name="username" id="username" tabindex="1" class="form-control" placeholder="Username1" value="">
                            </div>
                        </div>
                        <div class="input-wrap">
                            <div class="form-group">
                                <input type="password" name="password" id="password" tabindex="2" class="form-control" placeholder="Password">
                            </div>
                        </div>
                        <div class="input-wrap">
                            <div class="form-group">
                                <input type="password" name="confirm_password" id="confirm-password" tabindex="2" class="form-control" placeholder="Confirm Password">
                            </div>
                        </div>
                        <button type= "submit" formaction="/register" class="btn btn--blue btn--full">Create New Wallet </button>
                    </div>
                    <div id="tab-2" class="walletselect support__content-item support__desk">
